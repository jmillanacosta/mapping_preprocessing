SELECT ?primaryID ?secondaryID WITH {
SELECT ?primaryID
WHERE
{
{ ?primaryID p:P31/ps:P31 wd:Q7187 }
UNION
{ ?primaryID p:P31/ps:P31 wd:Q8054 }
UNION
{ ?primaryID p:P279/ps:P279 wd:Q7187 }
UNION
{ ?primaryID p:P279/ps:P279 wd:Q8054 }
}
} AS %RESULTS {
INCLUDE %RESULTS

        ?secondaryID owl:sameAs ?primaryID. 
}
